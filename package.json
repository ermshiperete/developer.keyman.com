{
  "name": "keyman-developer-online",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc --watch -p tsconfig.build.json",
    "gulp": "gulp",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "cross-env NODE_ENV=development  ts-node -r tsconfig-paths/register src/main.ts",
    "start:watch:dev": "cross-env NODE_ENV=development  nodemon",
    "start:watch:debug": "cross-env NODE_ENV=development  nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "cross-env NODE_ENV=production  node dist/main.js",
    "lint": "tslint -p tsconfig.lint.json -c tslint.json",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test  jest --coverage",
    "test:debug": "cross-env NODE_ENV=test  node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test  jest --verbose --config ./test/jest-e2e.json",
    "test:e2e:watch": "cross-env NODE_ENV=test  jest --verbose --config ./test/jest-e2e.json --watch --onlyChanged",
    "test:e2e:debug": "cross-env NODE_ENV=test  node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@nest-middlewares/cookie-parser": "^6.0.0",
    "@nest-middlewares/cookie-session": "^6.0.0",
    "@nest-middlewares/express-session": "^6.0.1",
    "@nest-middlewares/helmet": "^6.0.0",
    "@nestjs/common": "^6.11.8",
    "@nestjs/core": "^6.11.8",
    "@nestjs/platform-express": "^6.11.8",
    "@types/debug": "^4.1.5",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.11.0",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.3",
    "memorystore": "^1.6.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "simple-git": "^1.131.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.11.8",
    "@schematics/angular": "^8.3.25",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.2",
    "@types/express-rate-limit": "^3.3.4",
    "@types/express-session": "^1.15.16",
    "@types/hapi__joi": "^15.0.4",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.28",
    "@types/supertest": "^2.0.8",
    "axios-mock-adapter": "^1.17.0",
    "base-64": "^0.1.0",
    "cross-env": "^6.0.3",
    "gulp": "^4.0.2",
    "gulp-run-command": "0.0.9",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "supertest-session-promise": "^2.0.3",
    "ts-jest": "^24.3.0",
    "ts-mockito": "^2.5.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.2"
  },
  "jest": {
    "projects": [
      {
        "rootDir": "src",
        "moduleFileExtensions": [
          "js",
          "json",
          "ts"
        ],
        "testRegex": ".spec.ts$",
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "<rootDir>/../coverage",
        "testEnvironment": "node"
      }
    ]
  }
}
